    <script>
      document.addEventListener('click', function(e) {
        const btn = e.target.closest('.addtocartbtnDiv');
        if (!btn) return;

        // Wait for Shopify to add item to cart
        setTimeout(function() {
          fetch('/cart.js')
            .then(res => res.json())
            .then(cart => {
              const lastItem = cart.items[cart.items.length - 1];
              if (!lastItem) return;

              const ga4Item = {
                item_id: lastItem.product_id.toString(),
                item_name: lastItem.title,
                item_brand: lastItem.vendor || "",
                item_category: lastItem.product_type || "",
                item_category2: lastItem.collections?.[0]?.title || '',
                item_category3: lastItem.collections?.[1]?.title || '',
                item_category4: lastItem.collections?.[2]?.title || '',
                item_category5: lastItem.collections?.[3]?.title || '',
                item_list_id: 'cart',
                item_list_name: 'Cart',
                item_variant: lastItem.variant_title || "",
                price: lastItem.price / 100,
                quantity: lastItem.quantity,
                location_id: 'online_store',
                affiliation: "Online Store",
                discount: parseFloat(
                            cartLine.cost?.totalDiscountAmount?.amount || 0
                          )  
              };

              // Detect Incognito Mode
              detectIncognito().then((result) => {
                let incognito_windowValue = result.isPrivate ? 1 : 0;

                // GA4 event
                gtag('event', 'add_to_cart', {
                  incognito_window: incognito_windowValue,
                  currency: Shopify.currency.active,
                  value: (lastItem.price * lastItem.quantity) / 100,
                  items: [ga4Item]
                });

                console.log("GA4 add_to_cart event fired:", {
                  incognito_window: incognito_windowValue,
                  item: ga4Item
                });
              });

            });
        }, 300);
      });
    </script>

    {% comment %} Remove from cart {% endcomment %}
      <script>
        document.addEventListener('click', function(e) {

          // Detect REMOVE from cart buttons / icons
          const removeBtn = e.target.closest(
            '.cart-remove-button'
          );

          if (!removeBtn) return;

          // Get line item key (Shopify passes it in attribute)
          const lineKey = removeBtn.getAttribute('data-line-key')
            || removeBtn.getAttribute('data-line')
            || null;

          if (!lineKey) return;

          // Fetch cart BEFORE removal to get item details
          fetch('/cart.js')
            .then(res => res.json())
            .then(cart => {
              const item = cart.items.find(i => i.key === lineKey);
              if (!item) return;

              const removedItem = {
                item_id: lastItem.product_id.toString(),
                item_name: lastItem.title,
                item_brand: lastItem.vendor || "",
                item_category: lastItem.product_type || "",
                item_category2: lastItem.collections?.[0]?.title || '',
                item_category3: lastItem.collections?.[1]?.title || '',
                item_category4: lastItem.collections?.[2]?.title || '',
                item_category5: lastItem.collections?.[3]?.title || '',
                item_list_id: 'cart',
                item_list_name: 'Cart',
                item_variant: lastItem.variant_title || "",
                price: lastItem.price / 100,
                quantity: lastItem.quantity,
                location_id: 'online_store',
                affiliation: "Online Store",
                discount: parseFloat(
                            cartLine.cost?.totalDiscountAmount?.amount || 0
                          )  
              };

              // Detect Incognito Mode
              detectIncognito().then((result) => {
                let incognito_windowValue = result.isPrivate ? 1 : 0;

                gtag('event', 'remove_from_cart', {
                  incognito_window: incognito_windowValue,
                  currency: Shopify.currency.active,
                  value: (item.price * item.quantity) / 100,
                  items: [removedItem]
                });

                console.log("GA4 begin_checkout event fired:", {
                  incognito_window: incognito_windowValue,
                  items: removedItem
                });
              });

              console.log("GA4 remove_from_cart fired:", removedItem);
            });
        });
      </script>


      <script>
// ---------------------------
// Store predictive search term
// ---------------------------
document.addEventListener("DOMContentLoaded", function () {
  const searchInput = document.querySelector('input[name="q"], .predictive-search-form__header-inner input');

  if (searchInput) {
    // When user presses Enter
    searchInput.addEventListener("keydown", function (e) {
      if (e.key === "Enter") {
        sessionStorage.setItem("predictive_search_term", searchInput.value.trim());
      }
    });

    // When user clicks predictive search results
    document.addEventListener("click", function (e) {
      const resultItem = e.target.closest("[data-predictive-search-result]");
      if (resultItem && searchInput.value.trim()) {
        sessionStorage.setItem("predictive_search_term", searchInput.value.trim());
      }
    });
  }

  // ---------------------------
  // Fire GA4 event on next page
  // ---------------------------
  const storedSearchTerm = sessionStorage.getItem("predictive_search_term");

  if (storedSearchTerm) {
    sessionStorage.removeItem("predictive_search_term");

    // Fire GA Search event
    gtag('event', 'search', {
      search_term: storedSearchTerm
    });

    console.log("GA4 predictive search event fired:", storedSearchTerm);

    // Incognito mode detection
    detectIncognito().then((result) => {
      let incognito_windowValue = result.isPrivate ? 1 : 0;

      gtag('event', 'search', {
        search_term: storedSearchTerm,
        incognito_window: incognito_windowValue
      });

      console.log("GA4 search event + incognito:", {
        search_term: storedSearchTerm,
        incognito_window: incognito_windowValue
      });
    });
  }

  // ---------------------------
  // Normal Search Page Event
  // ---------------------------
  const urlParams = new URLSearchParams(window.location.search);
  const directSearchTerm = urlParams.get('q') || urlParams.get('_ss');

  if (window.location.pathname.includes('/search') && directSearchTerm) {
    gtag('event', 'search', {
      search_term: directSearchTerm
    });

    console.log("GA4 search event fired:", directSearchTerm);

    detectIncognito().then((result) => {
      let incognito_windowValue = result.isPrivate ? 1 : 0;

      gtag('event', 'search', {
        search_term: directSearchTerm,
        incognito_window: incognito_windowValue
      });

      console.log("GA4 search event + incognito:", {
        search_term: directSearchTerm,
        incognito_window: incognito_windowValue
      });
    });
  }

});
</script>

{% comment %} View Cart event for Cart Page + Cart Drawer {% endcomment %}
<script>
document.addEventListener("DOMContentLoaded", function () {

  function fireViewCartEvent(cart, source) {

        const items = cart.items.map((item, index) => ({
            item_id: item.product_id.toString(),
            item_name: item.title,
            item_brand: item.vendor || "",
            item_category: item.product_type || "",
            item_category2: item.collections?.[0]?.title || '',
            item_category3: item.collections?.[1]?.title || '',
            item_category4: item.collections?.[2]?.title || '',
            item_category5: item.collections?.[3]?.title || '',
            item_list_id: 'cart',
            item_list_name: 'Cart',
            item_variant: item.variant_title || "",
            price: item.price / 100,
            quantity: item.quantity,
            location_id: 'online_store',
            index: index + 1,
            affiliation: "Online Store",
            discount: parseFloat(
            cartLine.cost?.totalDiscountAmount?.amount || 0
          )  
       
      }));

    // Detect Incognito Mode
    detectIncognito().then((result) => {
      let incognito_windowValue = result.isPrivate ? 1 : 0;

      gtag("event", "view_cart", {
        incognito_window: incognito_windowValue,
        cart_source: source, 
        currency: Shopify.currency.active,
        value: cart.total_price / 100,
        items: items
      });

      console.log("GA4 view_cart event fired:", {
        source: source,
        incognito_window: incognito_windowValue,
        items: items
      });
    });
  }

  if (window.location.pathname === "/cart") {
    fetch("/cart.js")
      .then(res => res.json())
      .then(cart => fireViewCartEvent(cart, "cart_page"));
  }

  document.addEventListener("click", function (e) {
    const openCartDrawerBtn = e.target.closest(
      ".cart-drawer, .header-cart, .open-cart, button[data-cart-drawer], a[href='#cart'], .cart-toggle , .header_cart_content "
    );

    if (!openCartDrawerBtn) return;

    setTimeout(() => {
      fetch("/cart.js")
        .then(res => res.json())
        .then(cart => fireViewCartEvent(cart, "drawer"));
    }, 300);
  });

});
</script>

