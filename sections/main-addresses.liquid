										<td class="value"><input type="text" id="address_zip_{{form.id}}" class="address_form address_zip_edit" name="address[zip]" value="{{form.zip}}" size="40" /></td>
									</tr>
									<tr>
										<td class="label"><label for="address_phone_{{form.id}}">{{ 'customer.addresses.phone' | t }}</label></td>
										<td class="value"><input type="text" id="address_phone_{{form.id}}" class="address_form address_phone_edit" name="address[phone]" value="{{form.phone}}" size="40" /></td>
									</tr>
